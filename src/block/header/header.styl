.header
  flex-box(flex, row, space-around, center)
  font-size 30px
  font-family 'Raleway', sans-serif
  font-weight 900
  color #ffffff
  text-transform uppercase
  line-height 0.8
  text-align center
  letter-spacing 2.9px
  margin 92px 0 0 0

@media (max-width: 990px)
  .header
    padding 0 55px
    line-height 1.3
    font-size 25px
    font-weight 900
    text-transform uppercase
    text-align center
    letter-spacing 2.5px
    margin 43px 0 0 0